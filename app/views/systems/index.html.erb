<h1>Listing systems</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Location</th>
    <th>Description</th>
    <th>Contact</th>
    <th>Serialnumber</th>
    <th>Hardwareversion</th>
    <th>Softwareversion</th>
    <th>Dbversion</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @systems.each do |system| %>
  <tr>
    <td><%= system.name %></td>
    <td><%= system.location %></td>
    <td><%= system.description %></td>
    <td><%= system.contact %></td>
    <td><%= system.serialNumber %></td>
    <td><%= system.hardwareVersion %></td>
    <td><%= system.softwareVersion %></td>
    <td><%= system.dbVersion %></td>
    <td><%= link_to 'Show', system %></td>
    <td><%= link_to 'Edit', edit_system_path(system) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New System', new_system_path %>
